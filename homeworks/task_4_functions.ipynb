   "source": [
    "# Create a function to evalue the absolute value"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "b7f1c9ad",
   "metadata": {},
   "outputs": [],
   "source": [
    "def absolute_value (x):\n",
    "    if x < 0:\n",
    "        return -x\n",
    "    else: \n",
    "        return x    "
   ]
  },
